      "grid": 12,
      "rows": [
        {
          "label": "Full Width",
          "name": "Full Width",
          "areas": [
            {
              "grid": 12,
              "hasConfig": false,
              "controls": [
                {
                  "value": "<p style=\"text-align: left;\">Generate a DataTable from a SQL Query</p>",
                  "editor": {
                    "alias": "rte"
                  },
                  "active": false
                },
                {
                  "value": "<pre>Public Shared Function GetPollingCompanies() As DataTable\n    Dim dt As New DataTable\n    Dim strSQL As String = \"Select ID, PollerName FROM dbo.Pollers\"\n        Using conn As New SqlConnection(My.Settings.CS)\n            Using dad As New SqlDataAdapter(strSQL, conn)\n                dad.Fill(dt)\n            End Using\n        End Using\n    Return dt\nEnd Function</pre>",
                  "editor": {
                    "alias": "rte"
                  },
                  "active": false
                },
                {
                  "value": "<p>Execute a SQL Command</p>",
                  "editor": {
                    "alias": "rte"
                  },
                  "active": false
                },
                {
                  "value": "<pre>Public Shared Sub DeleteMyTable()\n    Dim strSQl As String = \"DROP TABLE MyTable\"\n    Using Conn As New SqlConnection(My.Settings.CS)\n        Using cmd As New SqlCommand(strSQL, Conn)\n            cmd.ExecuteNonQuery()\n        End Using\n    End Using\nEnd Function</pre>",
                  "editor": {
                    "alias": "rte"
                  },
                  "active": false
                },
                {
                  "value": "<p>Utilise SQLReader</p>",
                  "editor": {
                    "alias": "rte"
                  },
                  "active": false
                },
                {
                  "value": "<pre>Public Shared Function GetProjects() As List(Of ProjectItems)\n    Dim output As New List(Of ProjectItems)\n    Dim strSQl As String = \"SELECT ID, ProjectName, ProjectDirectory FROM Projects WHERE deleted='N'\"\n    Using conn As New SqlConnection(My.Settings.CS)\n        Using cmd As New SqlCommand(strSQl, conn)\n            Using RDR = cmd.ExecuteReader()\n                If RDR.HasRows Then\n                Do While RDR.Read\n                    Dim pi As New ProjectItems(CType(RDR.Item(\"id\"), Integer), RDR.Item(\"ProjectName\"), RDR.Item(\"ProjectDirectory\"))\n                    output.Add(pi)\n                Loop\n                End If\n            End Using\n        End Using\n    End Using\n    Return output\nEnd Function</pre>",
                  "editor": {
                    "alias": "rte"
                  },
                  "active": false
                }
              ]
            }
          ],
          "hasConfig": false,
          "id": "1cee74db-da39-1dbd-ca6c-e3cec65cff23"
        }
      ]
    }
  ]
}]]></contentGrid>
