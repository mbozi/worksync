@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
	Layout = "MenuPage.cshtml";
}
@{ Html.RenderAction("RenderMenuPageGallery", "Article"); }

@section StylesheetPlus
{
	<link rel="stylesheet" href="~/css/menu-article-gallery.css">
}

@section ScriptsEnd
{
	<script type="text/javascript">
        jQuery(document).ready(function ($) { $(".article-menu-item").click(function () { window.open($(this).data("href")); }); });
	</script>
}